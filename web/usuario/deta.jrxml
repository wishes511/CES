	<queryString>
		<![CDATA[SELECT m.nombre,isnull(p.nombre,'') as proveedor,a.nombre as 'area',d.nombre as 'depa',u.empresa as 'empresa',m.fecha,m.hora,m.tipo_mov,m.horasalida,m.tipo_mov,m.horasalida,'0'+cast((SUM(m.tiempo)/60)as varchar)+':'+cast((SUM(m.tiempo)%60) as varchar)+':00' as tiempo,m.observaciones,m.visita,m.asunto,m.tipo_transporte from movimiento m join departamento d on m.clave_departamento= d.clave_departamento
join area a on a.clave_area=d.clave_area
left join usuario u on m.clave_usuario =u.clave_usuario
left join proveedor p on m.clave_proveedor=p.clave_proveedor
where (m.nombre like '%$P!{nombre}%' or p.nombre like '%$P!{nombre}%') and m.area like '%$P!{area}%' and d.nombre like '%$P!{depa}%' and m.tipo_mov like '%$P!{tipo}%' and m.tipo_transporte like '%$P!{transporte}%' and m.visita like '%$P!{destino}%' and (m.tipo_persona!='U' and m.tipo_persona=$P{tipo_persona}) and m.fecha between $P{f1} and $P{f2}
 group by a.nombre,m.nombre,m.tipo_persona,m.visita,d.nombre,m.fecha,p.nombre,u.empresa,m.hora,m.tipo_mov,m.horasalida,m.tiempo,m.observaciones,m.asunto,m.tipo_transporte]]>
	</queryString>
